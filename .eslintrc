{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": [
    "standard",
    "standard-react"
  ],
  "globals": {
    "process": true,
    "module": true
  },
  "rules": {
    "react/prop-types": 0
  },
  "parserOptions": {
    "ecmaFeatures": {
      "legacyDecorators": true
    }
  }
}